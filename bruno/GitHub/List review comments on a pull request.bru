meta {
  name: List review comments on a pull request
  type: http
  seq: 6
}

get {
  url: https://api.github.com/repos/:owner/:repo/pulls/:pullNumber/comments
  body: none
  auth: inherit
}

params:path {
  owner: {{owner}}
  pullNumber: 1
  repo: docs-parser
}

headers {
  Accept: application/vnd.github+json
  X-GitHub-Api-Version: 2022-11-28
}

settings {
  encodeUrl: false
  timeout: 0
}
